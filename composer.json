{
  "name": "brick/varexporter",
  "description": "A pretty alternative to var_export(), which can export objects without __set_state()",
  "type": "library",
  "keywords": [
    "var_export"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "brick/reflection": "^0.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Brick\\VarExporter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\VarExporter\\Tests\\": "tests/"
    }
  }
}
